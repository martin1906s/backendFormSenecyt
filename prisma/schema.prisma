  Pais_Estudiante_paisResidenciaIdToPais                             Pais?                                      @relation("Estudiante_paisResidenciaIdToPais", fields: [paisResidenciaId], references: [id])
  Provincia_Estudiante_provinciaNacimientoIdToProvincia              Provincia?                                 @relation("Estudiante_provinciaNacimientoIdToProvincia", fields: [provinciaNacimientoId], references: [id])
