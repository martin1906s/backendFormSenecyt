generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Canton {
  id                                               String       @id
  nombre                                           String
  provinciaId                                      String
  codigo                                           Int          @unique
  Provincia                                        Provincia    @relation(fields: [provinciaId], references: [id])
  Estudiante_Estudiante_cantonNacimientoIdToCanton Estudiante[] @relation("Estudiante_cantonNacimientoIdToCanton")
  Estudiante_Estudiante_cantonResidenciaIdToCanton Estudiante[] @relation("Estudiante_cantonResidenciaIdToCanton")
  Colegio                                          Colegio[]

  @@unique([nombre, provinciaId])
}

model Colegio {
  id              String      @id @default(uuid())
  nombre          String
  sostenimiento   TipoColegio
  provinciaId     String
  cantonId        String
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
  Provincia       Provincia   @relation(fields: [provinciaId], references: [id])
  Canton          Canton      @relation(fields: [cantonId], references: [id])

  @@unique([nombre, cantonId])
  @@index([provinciaId])
  @@index([cantonId])
}

model ComposicionFamiliar {
  id               Int        @id @default(autoincrement())
  estudianteId     Int
  nombresApellidos String     @default("NA")
  fechaNacimiento  String     @default("NA")
  cedulaIdentidad  String     @default("NA")
  estadoCivil      String     @default("NA")
  parentesco       String     @default("NA")
  nivelEstudios    String     @default("NA")
  titulo           String     @default("NA")
  laborOcupacion   String     @default("NA")
  createdAt        DateTime   @default(now())
  updatedAt        DateTime   @updatedAt
  Estudiante       Estudiante @relation(fields: [estudianteId], references: [id], onDelete: Cascade)
}

