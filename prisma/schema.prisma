model IngresoFamiliar {
  id               Int        @id @default(autoincrement())
  estudianteId     Int
  nombresApellidos String     @default("NA")
  parentesco       String     @default("NA")
  actividadLaboral String     @default("NA")
  ingresoMensual   String     @default("NA")
  ingresosExtras   String     @default("NA")
  total            String     @default("NA")
  createdAt        DateTime   @default(now())
  updatedAt        DateTime   @updatedAt
  Estudiante       Estudiante @relation(fields: [estudianteId], references: [id], onDelete: Cascade)
}

model Pais {
  id                                             String       @id
  nombre                                         String       @unique
  codigo                                         Int          @unique
  Estudiante_Estudiante_paisNacionalidadIdToPais Estudiante[] @relation("Estudiante_paisNacionalidadIdToPais")
  Estudiante_Estudiante_paisResidenciaIdToPais   Estudiante[] @relation("Estudiante_paisResidenciaIdToPais")
  Provincia                                      Provincia[]
}

model Provincia {
  id                                                     String       @id
  nombre                                                 String
  paisId                                                 String
  codigo                                                 Int          @unique
  Canton                                                 Canton[]
  Colegio                                                Colegio[]
  Estudiante_Estudiante_provinciaNacimientoIdToProvincia Estudiante[] @relation("Estudiante_provinciaNacimientoIdToProvincia")
  Estudiante_Estudiante_provinciaResidenciaIdToProvincia Estudiante[] @relation("Estudiante_provinciaResidenciaIdToProvincia")
  Pais                                                   Pais         @relation(fields: [paisId], references: [id])

  @@unique([nombre, paisId])
}

model PuebloYNacionalidad {
  id                                                        String       @id
  codigo                                                    Int          @unique
  nombre                                                    String       @unique
  Estudiante_Estudiante_nacionalidadIdToPuebloYNacionalidad Estudiante[] @relation("Estudiante_nacionalidadIdToPuebloYNacionalidad")
  Estudiante_Estudiante_puebloIdToPuebloYNacionalidad       Estudiante[] @relation("Estudiante_puebloIdToPuebloYNacionalidad")
}

model SectorEconomico {
  id         String       @id
  nombre     String       @unique
  codigo     Int          @unique
  Estudiante Estudiante[]
}

